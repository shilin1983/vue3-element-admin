{
  "name": "vue3-element-admin",
  "version": "1.0.0",
  "description": "一套基于vue3、element-plus、typesScript、vite4的后台集成方案。",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "shilin",
    "email": "shilin_83@qq.com"
  },
  "engines": {
    "node": ">= 16.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shilin1983/vue3-element-admin.git"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:format": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
    "lint:style": "stylelint --fix \"**/*.{vue,less,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.2.4",
    "element-plus": "^2.2.28",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.29",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-types": "^5.0.2",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vueuse/core": "^9.11.1",
    "@zxcvbn-ts/core": "^2.1.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.14.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.3",
    "rollup": "^3.11.0",
    "stylelint": "^14.16.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.1",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-auto-import-element-plus-preset": "^1.0.4",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-setup-extend-plus": "^0.4.6",
    "vite": "^4.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^2.9.6",
